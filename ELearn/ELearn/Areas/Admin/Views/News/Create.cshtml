@model News
@{
    ViewData["Title"] = "Create";
}

<div class="container mt-5">
    <h1>Slider Create</h1>
    <form class="forms-sample">
        <div class="form-group mt-5">
            <label for="formFile" class="form-label">Choose a new image for news</label>
            <input class="form-control" type="file" id="formFile">
        </d iv>
        <di v class="form-group">
            <label for="exampleInputEmail1">Header</label>
            <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Header">
        </div>
       <div class="form-group">
            <label for="exampleInputEmail1">Time</label>
            <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Time">
        </div>
          <div class="form-group">
            <label for="exampleInputEmail1">Publisher FullName</label>
            <input type="email" class="form-control" id="exampleInputEmail1" placeholder="Publisher FullName">
        </div>

         <div class="buttons mt-3">
            <button type="submit" class="btn btn-primary mr-2">Submit</button>
            <button class="btn btn-dark">Cancel</button>
        </div>
    </form>
    <div class="back">
        <a class="btn btn-primary btn-rounded btn-fw my-5" asp-action="Index">

            Back to main page
        </a>
    </div>
</div>

